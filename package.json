{
  "name": "wapichan-map",
  "version": "1.0.0",
  "description": "Map of Wapichan monitoring points",
  "main": "src/index.js",
  "dependencies": {
    "d3-request": "^1.0.6",
    "date-fns": "^1.28.5",
    "mapbox-gl": "^0.40.0",
    "object-assign": "^4.1.1",
    "sheetify": "^6.0.1",
    "yo-yo": "^1.4.0"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "budo": "^10.0.4",
    "copyfiles": "^1.2.0",
    "es2020": "^1.1.9",
    "gh-pages": "^1.0.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "standard": "^10.0.2",
    "uglify-js": "^3.1.1",
    "uglifyify": "^4.0.3",
    "varify": "^0.2.0",
    "workbox-cli": "^2.0.1",
    "yo-yoify": "^3.7.0"
  },
  "scripts": {
    "start": "budo src/index.js:bundle.js --live -d static -- -t [ sheetify/transform ]",
    "lint": "standard",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && mkdirp dist && npm run build:js && npm run build:static && workbox generate:sw",
    "build:js": "browserify src/index.js -t [ sheetify/transform ] -t yo-yoify -t es2020 -g uglifyify | uglifyjs -c comparisons=false -m > dist/bundle.js",
    "build:static": "copyup 'static/**/*' dist",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "author": "Digital Democracy",
  "license": "MIT"
}
